{
  "name": "CS180 Extension",
  "description": "Productivity extension",
  "version": "0.1",
  "permissions": [
    "alarms",
    "geolocation",
    "identity",
    "storage",
    "http://cs180.no-ip.info/",
    "https://*.yahooapis.com/*"
  ],
  "browser_action": {
      "default_title": "New Tab",
      "default_icon": "src/img/icon.png"
  },
  "chrome_url_overrides" : {
    "newtab": "src/newtab.html"
  },
  "content_security_policy": "script-src https://*.yahooapis.com 'unsafe-eval'; object-src 'self'",
    
  "content_scripts": [
    {
        "js": ["src/js/weather.js", "src/js/jquery.simpleWeather.min.js"],
        "matches": ["*://*/*"],
        "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
